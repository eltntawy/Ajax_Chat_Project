<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.UserEntity" table="User" schema="" catalog="Chat_DB">
        <id name="id">
            <column name="id" sql-type="int" not-null="true"/>
        </id>
        <property name="fullName">
            <column name="fullName" sql-type="varchar" length="500"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar" length="500" not-null="true"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar" length="500" not-null="true"/>
        </property>
        <set name="userSendMessagesById" inverse="true">
            <key>
                <column name="fromUser" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.UserSendMessageEntity"/>
        </set>
        <set name="userSendMessagesById_0" inverse="true">
            <key>
                <column name="toUser" not-null="true"/>
            </key>
            <one-to-many not-found="ignore" class="com.UserSendMessageEntity"/>
        </set>
    </class>
</hibernate-mapping>